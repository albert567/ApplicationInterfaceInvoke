package com.gwamcc.aii.forms;

import com.gwamcc.aii.util.sql2.anno.PK;
import com.gwamcc.aii.util.sql2.anno.Table;

/**
 * 单独接口对象
 * @author 范大德
 *
 */
@Table("T_Interface")
public class SimpleInterForm extends DefaultForm {
	@PK
	private int id;//接口id
	private String name;//接口名称
	public SimpleInterForm() {}
	public int getId() {
		return id;
	}
	public SimpleInterForm setId(int id) {
		this.id = id;
		return this;
	}
	public String getName() {
		return name;
	}
	public SimpleInterForm setName(String name) {
		this.name = name;
		return this;
	}

}
