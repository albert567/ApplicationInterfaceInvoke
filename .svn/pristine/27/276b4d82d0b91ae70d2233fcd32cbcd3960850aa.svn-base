package com.gwamcc.aii.forms;

import com.gwamcc.aii.util.sql2.anno.PK;
import com.gwamcc.aii.util.sql2.anno.Table;

/**
 * 接口方法名称对象
 * @author 张亚平
 *
 */
@Table("T_InterfaceMethod")
public class MethodNameForm extends DefaultForm{
	@PK
	private int id;//主键ID
	private String name;//方法名称
	public MethodNameForm() {	}
	public int getId() {
		return id;
	}
	public MethodNameForm setId(int id) {
		this.id = id;
		return this;
	}
	public String getName() {
		return name;
	}
	public MethodNameForm setName(String name) {
		this.name = name;
		return this;
	}

}
