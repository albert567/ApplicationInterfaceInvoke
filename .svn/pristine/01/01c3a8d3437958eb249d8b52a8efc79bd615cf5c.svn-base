package com.gwamcc.aii.util.form.bean;

public class FormTitle {
	private String	title;
	private String	field;
	//private double	width;
	//private double	rowspan;
	//private double	colspan;
	//private String	align;
	//private String	halign;
	//private boolean	sortable;
	//private String	order;
	//private boolean	resizable;
	//private boolean	fixed;
	private boolean	hidden;
	private boolean	checkbox;
	private String	formatter;
	//private String	styler;
	//private String sorter;
	//private String editor;
	public String getTitle() {
		return title;
	}
	public FormTitle setTitle(String title) {
		if("undefined".equals(title)){
			return this;
		}
		this.title = title;
		return this;
	}
	public String getField() {
		return field;
	}
	public FormTitle setField(String field) {
		if("undefined".equals(field)){
			return this;
		}
		this.field = field;
		return this;
	}

	public boolean isHidden() {
		return hidden;
	}
	public FormTitle setHidden(boolean hidden) {
		this.hidden = hidden;
		return this;
	}
	public boolean isCheckbox() {
		return checkbox;
	}
	public FormTitle setCheckbox(boolean checkbox) {
		this.checkbox = checkbox;
		return this;
	}

	public String getFormatter() {
		return formatter;
	}
	public FormTitle setFormatter(String formatter) {
		if("undefined".equals(formatter)){
			return this;
		}
		this.formatter = formatter;
		return this;
	}

}
