#fauxconsole{
	position:absolute;
	top:0;
	right:0;
	width:300px;
	border:1px solid #999;
	font-family:courier,monospace;
	background:#eee;
	font-size:10px;
	padding:10px;
}
html>body #fauxconsole{
	position:fixed;
}
#fauxconsole a{
	float:right;
	padding-left:1em;
	padding-bottom:.5em;
	text-align:right;
}
