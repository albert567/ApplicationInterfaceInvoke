package com.gwamcc.aii.forms;

import com.gwamcc.aii.util.sql2.anno.Table;

/**
 * 接口名称对象
 * @author 范大德
 *
 */
@Table("T_Interface")
public class InterNameForm extends DefaultForm {
	private int id;//接口id
	private String name;//接口名称
	public InterNameForm() {	}
	public int getId() {
		return id;
	}
	public InterNameForm setId(int id) {
		this.id = id;
		return this;
	}
	public String getName() {
		return name;
	}
	public InterNameForm setName(String name) {
		this.name = name;
		return this;
	}
}
